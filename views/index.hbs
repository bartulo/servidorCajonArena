<!DOCTYPE html>
<script src="/socket.io/socket.io.js"></script>
  <script>
    const socket = io();
    document.querySelector('body').addEventListener('click', (e) => {
      const anchor = e.target.closest('a');
      if ( anchor !== null ) {
        socket.emit('openUrl', anchor.title);
      }
    },false);
socket.on( 'numClients', ( n ) => {
  console.log(n);
});
  </script>
<ul>
  {{#each datos}}
  <li>
    <a href="http://localhost:3000/visor/{{this.name}}" title={{ this.name }} target="_blank">{{this.name}}</a>
  </li>
  {{/each}}
</ul>
