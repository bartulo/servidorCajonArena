{{#if renderSidebar}}
  <div class="paint-container">
    <ul class="new-colors">
      <li data-color="#ffd700"></li>
      <li data-color="#9400d3"></li>
      <li data-color="#cd5c5c"></li>
      <li data-color="#ff8c00"></li>
      <li data-color="#adff2f"></li>
    </ul>
  </div>
  {{#if room}}
    <button class="textureButton">Topo</button>
    <div class="video-container">
      <div class="video-button"></div>
      <input class="new-seek" id="new-seek" value="0" min="0" type="range" step="1">
    </div>
  {{/if}}
  <div class="iconsToDelete"></div>
  <div class="linesToDelete"></div>
{{/if}}

<aside class="right-shelf">
  <section>
    {{>iconos}}
  </section>
  {{#if renderSidebar}}
    <div class="pull-tab">
      <div class="circle"></div>
      <div class="circle"></div>
      <div class="circle"></div>
    </div>
  {{/if}}
</aside>

<script src="/javascripts/app.bundle.js"></script>
